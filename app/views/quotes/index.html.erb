<div class="container">
  <div class="row">
    <div class="col-md-12">
      <% if @quotes.empty? %>
        No quotes added
      <%else%>
        <% @quotes.each do |quote| %>
          <div class="well well-lg">
            <div class="quote-body">
              <%= quote.text %>
            </div>
          </div>

        <% end %>
      <%end%>
    </div>
  </div>
</div>
